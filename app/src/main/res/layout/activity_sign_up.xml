<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.stammer.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_142pxv"
        android:layout_marginEnd="@dimen/_16pxh"
        android:layout_marginStart="@dimen/_16pxh"
        android:layout_marginTop="@dimen/_3pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyledeep_purple_A200">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSignUp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageButton
                    android:background="@drawable/rectangle_bg_deep_purple_a200_radius_15"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_44pxh"
                    android:paddingHorizontal="@dimen/_14pxh"
                    android:paddingVertical="@dimen/_14pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft_white_a700" tools:ignore="ContentDescription"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnsignUp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylecornerRadius">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumncreateanewac"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylecornerRadius">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtCreateaNewAc"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{signUpVM.signUpModel.txtCreateaNewAc}"
                            style="@style/txtInterbold32" tools:text="@string/msg_create_a_new_ac"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtItsfastande"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_15pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{signUpVM.signUpModel.txtItsfastande}"
                            style="@style/txtInterregular16" tools:text="@string/msg_it_s_fast_and_e"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumn"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_26pxv"
                            android:layout_width="match_parent"
                            android:orientation="vertical" style="@style/groupStylecornerRadius">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnframethirty"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylecornerRadius">
                                <LinearLayout
                                    android:id="@+id/linearRowframethirty"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
                                    <EditText
                                    android:background="@drawable/rectangle_bg_deep_purple_a100_radius_25"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_first_name"
                                    android:id="@+id/etFrameThirty"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.5"
                                    android:layout_width="0dp"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:paddingBottom="@dimen/_18pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_17pxv"
                                    android:singleLine="true"
                                    android:text="@={signUpVM.signUpModel.etFrameThirtyValue}"
                                    android:textColorHint="@color/white_A700"
                                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                                    <EditText
                                    android:background="@drawable/rectangle_bg_deep_purple_a100_radius_25"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_last_name"
                                    android:id="@+id/etFrameThirtyFour"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_weight="0.5"
                                    android:layout_width="0dp"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:paddingBottom="@dimen/_18pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_17pxv"
                                    android:singleLine="true"
                                    android:text="@={signUpVM.signUpModel.etFrameThirtyFourValue}"
                                    android:textColorHint="@color/white_A700"
                                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                                </LinearLayout>
                                <EditText
                                    android:background="@drawable/rectangle_bg_deep_purple_a100_radius_25"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_email_id"
                                    android:id="@+id/etFrameThirtyFive"
                                    android:inputType="textEmailAddress"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:paddingBottom="@dimen/_18pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_17pxv"
                                    android:singleLine="true"
                                    android:text="@={signUpVM.signUpModel.etFrameThirtyFiveValue}"
                                    android:textColorHint="@color/white_A700"
                                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                                <EditText
                                    android:background="@drawable/rectangle_bg_deep_purple_a100_radius_25"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_phone_no"
                                    android:id="@+id/etFrameThirtySix"
                                    android:inputType="phone"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:paddingBottom="@dimen/_18pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_17pxv"
                                    android:singleLine="true"
                                    android:text="@={signUpVM.signUpModel.etFrameThirtySixValue}"
                                    android:textColorHint="@color/white_A700"
                                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                                <EditText
                                    android:background="@drawable/rectangle_bg_deep_purple_a100_radius_25"
                                    android:drawableEnd="@drawable/img_calendar"
                                    android:drawablePadding="@dimen/_30pxh"
                                    android:ellipsize="end"
                                    android:hint="@string/lbl_date_of_birth"
                                    android:id="@+id/etFrameThirtySeven"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_16pxv"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:paddingBottom="@dimen/_18pxv"
                                    android:paddingEnd="@dimen/_18pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_17pxv"
                                    android:singleLine="true"
                                    android:text="@={signUpVM.signUpModel.etFrameThirtySevenValue}"
                                    android:textColorHint="@color/white_A700"
                                    style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                            </LinearLayout>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumngender"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_25pxv"
                                android:layout_width="match_parent"
                                android:orientation="vertical" style="@style/groupStylecornerRadius_3">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtGender"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{signUpVM.signUpModel.txtGender}"
                                    style="@style/txtIntermedium16" tools:text="@string/lbl_gender"/>
                                <RadioGroup
                                    android:id="@+id/rgRadiogroupfemale"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_13pxv"
                                    android:layout_width="match_parent"
                                    android:orientation="horizontal" style="@style/groupStylecornerRadius_2">
                                    <RadioButton
                                    android:buttonTint="@color/white_A700"
                                    android:ellipsize="end"
                                    android:fontFamily="@font/inter"
                                    android:id="@+id/rbFemale"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:paddingBottom="@dimen/_18pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_17pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_female"
                                    android:textColor="@color/white_A700"
                                    android:textFontWeight="500"
                                    android:textSize="@dimen/_14pxh"
                                    android:textStyle="bold" tools:text="@string/lbl_female"/>
                                    <RadioButton
                                    android:buttonTint="@color/white_A700"
                                    android:ellipsize="end"
                                    android:fontFamily="@font/inter"
                                    android:id="@+id/rbMale"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_width="wrap_content"
                                    android:lineSpacingExtra="@dimen/_1pxh"
                                    android:paddingBottom="@dimen/_18pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_16pxh"
                                    android:paddingTop="@dimen/_17pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_male"
                                    android:textColor="@color/white_A700"
                                    android:textFontWeight="500"
                                    android:textSize="@dimen/_14pxh"
                                    android:textStyle="bold" tools:text="@string/lbl_male"/>
                                </RadioGroup>
                            </LinearLayout>
                        </LinearLayout>
                        <TextView android:gravity="center_horizontal"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_marginTop="@dimen/_43pxv"
                            android:layout_width="match_parent"
                            android:text="@{signUpVM.signUpModel.txtLanguage}"
                            style="@style/txtInterregular14" tools:text="@string/msg_by_clicking_reg"/>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnSignUp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_57pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:paddingBottom="@dimen/_16pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_16pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_sign_up"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_sign_up"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
