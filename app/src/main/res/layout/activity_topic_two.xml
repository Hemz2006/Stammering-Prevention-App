<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="topicTwoVM" type="com.stammer.app.modules.topictwo.data.viewmodel.TopicTwoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackarrowleft"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageImageTwo"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_image2_852x407}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image2_852x407"/>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_19pxv"
            android:layout_marginEnd="@dimen/_4pxh"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_5pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <LinearLayout android:id="@+id/linearRowreply"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_290pxh"
                        android:layout_weight="0.82"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageReply"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_reply"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_reply"/>
                        <ImageView android:id="@+id/imageGroup8916"
                            android:layout_height="@dimen/_5pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_group8916"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group8916"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnscreen"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_70pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <View android:background="@color/bluegray_100"
                    android:id="@+id/viewSCREEN"
                    android:layout_height="@dimen/_196pxv" android:layout_width="@dimen/_398pxh"/>
                <LinearLayout android:id="@+id/linearRowrectangle3816"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnrectangle3816"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_224pxv"
                        android:layout_weight="0.45"
                        android:layout_width="0dp" android:orientation="vertical">
                        <FrameLayout
                            android:id="@+id/frameStackrectangle3816"
                            android:layout_height="@dimen/_216pxv" android:layout_width="@dimen/_170pxh">
                            <ImageView
                                android:id="@+id/imageRectangle3816"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_216pxh"
                                android:layout_width="@dimen/_170pxh"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_rectangle3816}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3816"/>
                            <TextView android:gravity="top|start"
                                android:id="@+id/txtPrice"
                                android:layout_gravity="top|start"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_16pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_16pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:text="@{topicTwoVM.topicTwoModel.txtPrice}"
                                style="@style/txtInterregular12" tools:text="@string/msg_this_course_wil2"/>
                        </FrameLayout>
                        <EditText
                            android:background="@drawable/rectangle_bg_red_a400"
                            android:ellipsize="end"
                            android:hint="@string/lbl_end_course"
                            android:id="@+id/etPriceOne"
                            android:inputType="text"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_24pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_20pxv"
                            android:singleLine="true"
                            android:text="@={topicTwoVM.topicTwoModel.etPriceOneValue}"
                            android:textColorHint="@color/white_A700"
                            style="@style/etSolid" tools:ignore="Autofill"/>
                    </LinearLayout>
                    <FrameLayout
                        android:id="@+id/frameStackrectangle3815"
                        android:layout_height="@dimen/_514pxv"
                        android:layout_marginStart="@dimen/_12pxh" android:layout_width="@dimen/_209pxh">
                        <ImageView android:id="@+id/imageRectangle3815"
                            android:layout_gravity="start"
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_rectangle3816}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle3816"/>
                        <LinearLayout
                            android:id="@+id/linearColumntopicone"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_25pxv"
                            android:layout_marginEnd="@dimen/_12pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_25pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnTopicOne"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_15pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_15pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_topic_1"
                                android:textAllCaps="false"
                                style="@style/btnSolid" tools:text="@string/lbl_topic_1"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnTopicThree"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_27pxv"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_15pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_15pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_topic_3"
                                android:textAllCaps="false"
                                style="@style/btnSolid" tools:text="@string/lbl_topic_3"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnTopicFour"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_29pxv"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_15pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_15pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_topic_4"
                                android:textAllCaps="false"
                                style="@style/btnSolid" tools:text="@string/lbl_topic_4"/>
                            <androidx.appcompat.widget.AppCompatButton
                                android:ellipsize="end"
                                android:gravity="center"
                                android:id="@+id/btnTopicFive"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_29pxv"
                                android:layout_width="match_parent"
                                android:paddingBottom="@dimen/_15pxv"
                                android:paddingEnd="@dimen/_30pxh"
                                android:paddingStart="@dimen/_30pxh"
                                android:paddingTop="@dimen/_15pxv"
                                android:singleLine="true"
                                android:text="@string/lbl_topic_5"
                                android:textAllCaps="false"
                                style="@style/btnSolid" tools:text="@string/lbl_topic_5"/>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
