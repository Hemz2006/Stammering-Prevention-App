<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homePageVM" type="com.stammer.app.modules.homepage.data.viewmodel.HomePageVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumngroup9020"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_5pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearHOMEPAGE"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.AppCompatButton
                    android:drawableEnd="@drawable/img_search"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnGroup9020"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_323pxh"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_16pxh"
                    android:paddingStart="@dimen/_16pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:textAllCaps="false" style="@style/btnSolidRounded_1"/>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnwelcome"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_50pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylepurple_A700cornerRadius">
                    <TextView android:ellipsize="end"
                        android:gravity="end"
                        android:id="@+id/txtWELCOME"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_56pxv"
                        android:layout_marginEnd="@dimen/_62pxh"
                        android:layout_marginStart="@dimen/_62pxh"
                        android:layout_marginTop="@dimen/_56pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{homePageVM.homePageModel.txtWELCOME}"
                        style="@style/txtIntersemibold36" tools:text="@string/lbl_welcome2"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnstammeringcure"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_5pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylepurple_100cornerRadius">
                    <FrameLayout
                        android:id="@+id/frameStackstammeringcure"
                        android:layout_height="@dimen/_202pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_44pxv" android:layout_width="@dimen/_317pxh">
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtSTAMMERINGCURE"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{homePageVM.homePageModel.txtSTAMMERINGCURE}"
                            style="@style/txtInterextrabold32" tools:text="@string/lbl_stammering_cure"/>
                        <ImageView android:id="@+id/imageImageremovebg"
                            android:layout_gravity="end"
                            android:layout_height="@dimen/_202pxh"
                            android:layout_marginEnd="@dimen/_28pxh"
                            android:layout_marginStart="@dimen/_28pxh"
                            android:layout_width="@dimen/_222pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_imageremovebg}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_imageremovebg"/>
                    </FrameLayout>
                    <TextView android:id="@+id/txtDescription"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_22pxv"
                        android:layout_marginEnd="@dimen/_14pxh"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_width="match_parent"
                        android:text="@{homePageVM.homePageModel.txtDescription}"
                        style="@style/txtInterregular16_1" tools:text="@string/msg_the_perfect_cou"/>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700cornerRadius">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnhome"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home_23x22"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home_23x22"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtHomeOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{homePageVM.homePageModel.txtHomeOne}"
                        style="@style/txtInterregular10_1" tools:text="@string/lbl_home"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnuser"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_21pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{homePageVM.homePageModel.txtProfile}"
                        style="@style/txtInterregular10" tools:text="@string/lbl_profile"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
